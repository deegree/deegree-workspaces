<deegreeWFS xmlns="http://www.deegree.org/services/wfs" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" configVersion="3.1.0" xsi:schemaLocation="http://www.deegree.org/services/wfs http://schemas.deegree.org/services/wfs/3.1.0/wfs_configuration.xsd">
  <SupportedVersions>
    <Version>1.1.0</Version>
  </SupportedVersions>
  <FeatureStoreId>cite110</FeatureStoreId>
  <EnableTransactions>true</EnableTransactions>
  <EnableResponseBuffering>true</EnableResponseBuffering>
  
  <QueryCRS>urn:ogc:def:crs:OGC:1.3:CRS84</QueryCRS>
  <QueryCRS>urn:ogc:def:crs:EPSG::4326</QueryCRS>
  <QueryCRS>EPSG:4326</QueryCRS>
  <QueryCRS>CRS:84</QueryCRS>
  
  <QueryCheckAreaOfUse>true</QueryCheckAreaOfUse>
  <GMLFormat gmlVersion="GML_31">
    <MimeType>text/xml; subtype=gml/3.1.1</MimeType>
    <GetFeatureResponse>
      <DisableStreaming>true</DisableStreaming>
    </GetFeatureResponse>
  </GMLFormat>
  <Strict>true</Strict>
</deegreeWFS>